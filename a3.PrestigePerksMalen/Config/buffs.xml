		<buff name="prestigeBuff" name_key="prestige" description_key="PPdesc" icon="ui_game_symbol_star" remove_on_death="false" icon_color="240,234,76">
			<stack_type value="ignore"/>
			<effect_group>
				<passive_effect name="PlayerExpGain" operation="perc_add" value="-.5"/>
				<triggered_effect trigger="onSelfBuffStart" action="RemoveBuff" buff="buffxpst"/>
