				<passive_effect name="PlayerExpGain" operation="perc_add" value="-.375">
					<requirement name="HasBuff" buff="OP3!"/>
				</passive_effect>
				<passive_effect name="PlayerExpGain" operation="perc_add" value="-.0375" tags="Kill">
					<requirement name="ProgressionLevel" progression_name="perkNightStalkerTwilightThief" operation="Equals" value="1"/>
				</passive_effect>				
			</effect_group>
			<effect_group>
				<requirement name="CVarCompare" cvar="$chale" operation="GT" value="-1"/>
				<triggered_effect trigger="onSelfDied" target="self" action="AddBuff" buff="buffPrestigeReset"/>										
			</effect_group>			
		</buff>

		<buff name="buffPrestigeReset" name_key="buffPrestigeResetName" description_key="buffPrestigeResetDesc" icon="ui_game_symbol_skull" remove_on_death="false" icon_color="0,176,246">
			<stack_type value="ignore"/>
			<duration value="3"/>
			<effect_group>	
				<triggered_effect trigger="onSelfBuffStart" action="ResetProgression" reset_skills="true" reset_levels="true" reset_books="false" reset_crafting="false"/>
				<triggered_effect trigger="onSelfBuffStart" action="ModifyCVar" cvar="$PlayerLevelBonus" operation="set" value="0"/>
				<triggered_effect trigger="onSelfBuffStart" action="ModifyCVar" cvar="$LastPlayerLevel" operation="set" value="0"/>
				<triggered_effect trigger="onSelfBuffStart" action="AddBuff" buff="buffPerkAbilityUpdate"/>
				<triggered_effect trigger="onSelfBuffStart" action="AddBuff" buff="buffDeathFoodDrinkAdjust"/>		

				<triggered_effect trigger="onSelfBuffRemove" action="ResetProgression" reset_skills="true" reset_levels="true" reset_books="false" reset_crafting="false"/>
				<triggered_effect trigger="onSelfBuffRemove" action="ModifyCVar" cvar="$PlayerLevelBonus" operation="set" value="0"/>
