<configs>

<append xpath="/windows">

  <window name="browserDiscord" controller="ServerInfo">
      <panel name="header" pos="-300,0" height="40" depth="1" backgroundspritename="ui_game_panel_header" >
          <label style="header.name" pos="0,0" width="197" justify="center" text="Discord Link" />
      </panel>
      <panel name="" pos="-300,0" height="63">
          <sprite depth="5" pos="0,0" height="33" width="200" name="background" color="[darkGrey]" type="sliced" />
          <label name="ServerDescription" />
          <label depth="2" pos="0,-40" height="32" width="200" name="ServerWebsiteURL" text="https://discord.gg/XM58vnbZJD" justify="center" style="press,hover" font_size="1" upper_case="false" sound="[paging_click]" />
          <sprite depth="3" pos="0,-40" height="32" width="200" name="URLMask" color="[white]" foregroundlayer="true" fillcenter="true" />
          <sprite depth="4" name="microphoneIcon" style="icon30px" pos="5,-40" color="[black]" sprite="ui_game_symbol_mic" />
          <label depth="4" style="header.name" pos="0,-40" height="32" width="200" justify="center" color="[black]" text="Click Here" />
      </panel>
  </window>

