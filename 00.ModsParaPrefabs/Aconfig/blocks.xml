<MalenEI>
	<append xpath="/blocks">

		<block name="fusebox">
			<property name="CreativeMode" value="None"/>
			<property name="DescriptionKey" value="awningGroupDesc"/>
			<property name="DisplayType" value="blockHardened"/>
			<property name="Material" value="MclothStable"/>
			<property name="Shape" value="New"/>
			<property name="Model" value="Cube"/>
			<property name="Texture" value="14"/>
			<property name="FuelValue" value="150"/>
			<property name="EconomicValue" value="25"/>
			<property name="FallDamage" value=".5"/>
			<property class="RepairItems">
				<property name="resourceCloth" value="8"/>
				<property name="resourceWood" value="2"/>
			</property>
			<drop event="Harvest" name="resourceCloth" count="3" tag="allHarvest"/>
			<drop event="Destroy" name="resourceCloth" count="1"/>
			<drop event="Fall" name="resourceCloth" count="1" prob="0.3" stick_chance="0"/>
			<property name="SortOrder1" value="M160"/>
			<property name="SortOrder2" value="0000"/>
			<property name="Group" value="Building,advBuilding"/>
			<property name="Tags" value="wood"/>
			<property name="FilterTags" value="MC_building"/>
		</block>
		<block name="cntTrashCompactor">
			<property name="Class" value="Loot"/>
			<property name="Shape" value="New"/>
			<property name="Path" value="solid"/>
			<property name="Model" value="counter"/>
			<property name="Place" value="TowardsPlacerInverted"/>
			<property name="IsDecoration" value="true"/>
			<property name="Material" value="Mmetal"/>
			<property name="Texture" value="191,191,60,191,191,191"/>
			<property name="LootList" value="trashCompactor"/>
			<property class="RepairItems">
				<property name="resourceForgedIron" value="5"/>
				<property name="resourceMechanicalParts" value="2"/>
				<property name="resourceElectricParts" value="1"/>
			</property>
			<drop event="Harvest" name="terrStone" count="0" tool_category="Disassemble"/>
			<drop event="Harvest" name="resourceMechanicalParts" count="1,3" tag="salvageHarvest"/>
			<drop event="Harvest" name="resourceElectricParts" count="1" tag="salvageHarvest"/>
			<drop event="Destroy" name="resourceScrapIron" count="5"/>
			<drop event="Fall" name="scrapMetalPile" count="1" prob="0.75" stick_chance="1"/>
			<property name="EconomicValue" value="60"/>
			<!-- disassemble value -->
			<property name="ImposterExchange" value="imposterBlock" param1="77"/>
			<property name="FilterTags" value="MC_building,SC_loot,SC_electrical"/>
		</block>
		<block name="mirror">
			<property name="CreativeMode" value="Dev"/>
			<property name="Material" value="Mglass"/>
			<property name="Shape" value="New"/>
			<property name="Model" value="mirror"/>
			<property name="Place" value="TowardsPlacerInverted"/>
			<property name="Texture" value="241,241,358,241,62,241"/>
			<property name="UseGlobalUV" value="Local"/>
			<drop event="Destroy" name="resourceBrokenGlass" count="1,3"/>
			<drop event="Fall" count="0" prob="0"/>
			<property name="DescriptionKey" value="furnitureGroupDesc"/>
			<property name="FilterTags" value="MC_building,SC_residential,SC_commercial,SC_plumbing"/>
		</block>

		<block name="tv_large_1">
			<property name="Extends" value="tv"/>
			<property name="CustomIcon" value="tv"/>
			<property name="Model" value="Furniture/tv_large_1" param1="main_mesh"/>
		</block>

		<!-- NOT_A_SHAPE -->
		<block name="tv_large_2">
			<property name="Extends" value="tv"/>
			<property name="CustomIcon" value="tv"/>
			<property name="Model" value="Furniture/tv_large_2" param1="main_mesh"/>
		</block>

		<block name="satelliteDish">
			<property name="Material" value="Mmetal_thin"/>
			<property name="Shape" value="ModelEntity"/>
			<property name="Path" value="solid"/>
			<property name="Model" value="Entities/Electrical/satellite_dishPrefab"/>
			<property name="HandleFace" value="Bottom"/>
			<property name="DisplayType" value="blockMulti"/>
			<property name="MultiBlockDim" value="1,2,1"/>
			<property name="StabilitySupport" value="false"/>
			<!-- build restriction -->
			<property name="ImposterDontBlock" value="true"/>
			<property name="Collide" value="movement,melee,bullet,arrow,rocket"/>
			<drop event="Harvest" name="terrStone" count="0" tool_category="Disassemble"/>
			<drop event="Harvest" name="resourceElectricParts" count="2,6" tag="salvageHarvest"/>
			<!-- HV: mounted fluorescent lamp -->
			<drop event="Harvest" name="resourceScrapPolymers" count="1,2" tag="salvageHarvest"/>
			<drop event="Destroy" name="resourceScrapIron" count="5"/>
			<drop event="Fall" name="scrapMetalPile" count="1" prob="0.75" stick_chance="1"/>
			<property name="Group" value="Science"/>
			<property name="EconomicValue" value="175"/>
			<property name="FilterTags" value="MC_building,SC_electrical,MC_outdoor,SC_decor"/>
			<property name="SortOrder1" value="70a0"/>
		</block>

		<!-- NOT_A_SHAPE -->
		<block name="satelliteDishSmall">
			<property name="Extends" value="satelliteDish" param1="MultiBlockDim"/>
			<property name="Model" value="Entities/Electrical/satellite_dish_smallPrefab"/>
			<property name="HandleFace" value="North"/>
			<drop event="Harvest" name="terrStone" count="0" tool_category="Disassemble"/>
			<drop event="Harvest" name="resourceElectricParts" count="2,6" tag="salvageHarvest"/>
			<!-- HV: mounted fluorescent lamp -->
			<drop event="Harvest" name="resourceScrapPolymers" count="1,2" tag="salvageHarvest"/>
		</block>

		<block name="doorFrameMetal1">
			<property name="Extends" value="corrugatedMetalNoUpgradeMaster"/>
			<property name="Shape" value="Ext3dModel"/>
			<property name="Texture" value="293"/>
			<property name="Mesh" value="models"/>
			<property name="Model" value="Buildings/door_frame_metal1" param1="main_mesh"/>
			<property name="Place" value="TowardsPlacer"/>
			<property name="Collide" value="melee,bullet,arrow,rocket"/>
			<property name="LightOpacity" value="1"/>
			<property class="UpgradeBlock">
				<property name="ToBlock" value="doorFrameMetalBoarded1"/>
				<property name="Item" value="resourceWood"/>
				<property name="ItemCount" value="4"/>
				<property name="UpgradeHitCount" value="4"/>
			</property>
			<property name="FilterTags" value="MC_building,SC_doors"/>
			<property name="SortOrder1" value="7060"/>
			<property name="SortOrder2" value="0700"/>
			<drop event="Fall" count="0" prob="0"/>
		</block>
		<block name="doorFrameMetalBoarded1">
			<property name="Extends" value="doorFrameWhiteBoarded1"/>
			<property name="Model" value="Buildings/door_frame_metal_boarded1" param1="main_mesh"/>
			<property name="DowngradeBlock" value="doorFrameMetal1"/>
		</block>

		<!-- NOT_A_SHAPE -->
		<block name="doorFrameMetal2">
			<property name="Extends" value="doorFrameMetal1"/>
			<property name="Model" value="Buildings/door_frame_metal2" param1="main_mesh"/>
			<property class="UpgradeBlock">
				<property name="ToBlock" value="doorFrameMetalBoarded2"/>
			</property>
		</block>


	</append>

</MalenEI>
