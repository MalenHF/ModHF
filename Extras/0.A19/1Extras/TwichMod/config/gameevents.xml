<config>
    <append xpath="gameevents">
        <!--Eventos-->
        <action_sequence name="spawn_boss">

            <action class="SpawnEntity">
                <property name="entity_names" value="BossTavoSoldier,BosszombieSiren,BossZombieJuggernaut,BossMineralSpider,BossMummy,BossPractoSpider" />
            </action>

            <action class="PlaySound">
                <property name="sound" value="twitch_bad_spawn" />
                <property name="phase" value="1" />
            </action>

            <action class="PlaySound">
                <property name="sound" value="zombiemalealert" />
                <property name="phase" value="1" />
            </action>

        </action_sequence>

        <action_sequence name="spawn_siren">

            <action class="SpawnEntity">
                <property name="entity_names" value="BosszombieSiren" />
            </action>

            <action class="PlaySound">
                <property name="sound" value="sirenidle" />
                <property name="phase" value="1" />
            </action>

            <action class="PlaySound">
                <property name="sound" value="zombiemalealert" />
                <property name="phase" value="1" />
            </action>

        </action_sequence>

        <action_sequence name="spawn_elite">

            <action class="SpawnEntity">
                <property name="entity_names" value="EliteCop,EliteSolider,EliteHawai,EliteSkater,EliteFeral,EliteBoomer,ZombieBomberElite" />
            </action>

            <action class="PlaySound">
                <property name="sound" value="twitch_bad_spawn" />
                <property name="phase" value="1" />
            </action>

            <action class="PlaySound">
                <property name="sound" value="zombiemalealert" />
                <property name="phase" value="1" />
            </action>

        </action_sequence>

        <action_sequence name="spawn_special">

            <action class="SpawnEntity">
                <property name="entity_names" value="HeadZombiePonky,VenSpiritu,NighScreamer,zombieGunner,TAVOzombieFire,MiniSiren" />
            </action>

            <action class="PlaySound">
                <property name="sound" value="twitch_bad_spawn" />
