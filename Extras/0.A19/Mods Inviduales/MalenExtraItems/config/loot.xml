<MalenEI>

	<!-- MalenEI Edits Start -->
	<append xpath="/lootcontainers">
		<lootqualitytemplates>
			<lootqualitytemplate name="speciallootmod">
				<qualitytemplate level="1,999999" default_quality="6">
					<loot quality="6" prob="1"/>
				</qualitytemplate>
			</lootqualitytemplate>
		</lootqualitytemplates>

		<lootgroup name="piezasarmas">
			<item name="gunRifleT1HuntingRifleParts" count="5,10"/>
			<item name="gunShotgunT1DoubleBarrelParts" count="5,10"/>
			<item name="gunMGT1AK47Parts" count="5,10"/>
			<item name="gunHandgunT1PistolParts" count="5,10"/>
			<item name="gunBowT1WoodenBowParts" count="5,10"/>
		</lootgroup>
		<lootgroup name="oresraros">
			<item name="resourceRawDiamond" count="5"/>
			<item name="resourceGoldNugget" count="10"/>
			<item name="resourceSilverNugget" count="15"/>
		</lootgroup>
		<lootgroup name="lootdebosspecial" count="all">
			<item name="resourceBrokenGlass" count="1"/>
			<item name="resourceSteelPolishR" count="10,20" />
			<item name="oldCash" count="50,150"/>
			<item group="piezasarmas" count="1"/>
			<!--<item name="eventIT1" count="1,5"/>-->
		</lootgroup>
		<lootgroup name="lootdebosspecial2" count="all">
			<item name="resourceBrokenGlass" count="1"/>
			<item name="resourceSteelPolishR" count="1,10" />
			<item name="oldCash" count="50,150"/>
			<item group="piezasarmas" count="1"/>
			<!--<item name="eventIT1" count="1,5"/>-->
		</lootgroup>
		<lootgroup name="lootdebosses" count="all">
			<item name="resourceBrokenGlass" count="1"/>
			<item name="resourceSteelPolishR" count="5,15" />
			<item name="Practonium" count="1"/>
			<item group="piezasarmas" count="1,2"/>
			<item name="oldCash" count="100,300"/>
			<item group="oresraros"/>
			<!--<item name="eventIT1" count="3,5"/>-->
		</lootgroup>
		<lootcontainer id="200" count="0" size="12,12" sound_open="UseActions/open_chest" sound_close="UseActions/close_chest" loot_quality_template="qualBaseTemplate">
			<item name="cobweb" count="1,2"/>
		</lootcontainer>
		<lootcontainer id="202" count="0" size="10,10" sound_open="UseActions/open_chest" sound_close="UseActions/close_chest" loot_quality_template="qualBaseTemplate">
			<item name="cobweb" count="1,2"/>
		</lootcontainer>

		<lootcontainer id="400" count="1" size="5,2" destroy_on_close="false" sound_open="UseActions/open_backpack" sound_close="UseActions/close_backpack" open_time="1" loot_quality_template="speciallootmod">
			<item group="lootdebosses" count="1"/>
		</lootcontainer>
		<lootcontainer id="401" count="1" size="5,2" destroy_on_close="false" sound_open="UseActions/open_backpack" sound_close="UseActions/close_backpack" open_time="1" loot_quality_template="speciallootmod">
			<item group="lootdebosspecial" count="1"/>
			<item name="Practonium" count="1" prob="0.25"/>
		</lootcontainer>
		<lootcontainer id="402" count="1" size="5,2" destroy_on_close="false" sound_open="UseActions/open_backpack" sound_close="UseActions/close_backpack" open_time="1" loot_quality_template="speciallootmod">
			<item group="lootdebosspecial2" count="1"/>
			<item name="Practonium" count="1" prob="0.25"/>
		</lootcontainer>
	</append>
	<!--Los dos nuevos Dye-->
	<append xpath="/lootcontainers/lootgroup[@name='dyes']">
		<item name="Dye: Silver"/>
		<item name="Dye: Candy Pink"/>
	</append>
	<!-- MalenEI Edits End -->
</MalenEI>
